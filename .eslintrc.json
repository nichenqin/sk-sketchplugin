{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "sketch"],
  "plugins": ["html"],
  "rules": {
    "no-console": 0,
    "arrow-parens": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-mixed-operators": 0,
    "max-len": "warn",
    "object-curly-newline": 0
  }
}
