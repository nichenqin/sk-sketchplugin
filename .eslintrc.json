{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "plugin:vue/recommended"],
  "settings": {
    "import/resolve": "webpack"
  },
  "rules": {
    "no-console": 0,
    "arrow-parens": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0
  }
}
